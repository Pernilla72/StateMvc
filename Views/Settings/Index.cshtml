@using StateMvc.Views.Settings
@model UpdateVM

@{
    ViewBag.Title = "Index";
}


<h1>Settings</h1>

@if (ViewBag.SuccessMessage != null)
{
    <div class="alert alert-success">@ViewBag.SuccessMessage</div>
}

<div>
    <strong>Support E-mail:</strong> @Model.Email
</div>
<div>
    <strong>Company name:</strong> @Model.CompanyName
</div>